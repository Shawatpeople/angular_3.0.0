{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^7.4",
        "apility/laravel-fcm": "^1.4",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "^2.12.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/mongodb": "3.7",
        "laravel/framework": "^7.0",
        "laravel/passport": "~9.0",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "3.4.6",
        "league/oauth2-server": "^8.3",
        "maatwebsite/excel": "^3.1",
        "metalback/tad-php": "^0.4.2",
        "mongodb/mongodb": "^1.12",
        "nwidart/laravel-modules": "8.0",
        "league/flysystem-sftp": "~1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "jason-guru/laravel-make-repository": "^0.0.3",
        "kitloong/laravel-migrations-generator": "^5.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
        "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/RouteHelper.php",
            "app/Helpers/CustomValidate.php",
            "app/Helpers/Helpers.php",
            "app/Helpers/LeaveHelpers.php",
            "app/Helpers/CommonHelper.php",
            "app/Helpers/MultiApprovalHelpers.php",
            "app/Helpers/MenuHelper.php",
            "Modules/ExitManagement/Helpers/CommonHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
